{
  "name": "redux-decorators",
  "version": "0.4.3",
  "scripts": {
    "build": "webpack",
    "postinstall": "typings install",
    "test": "mocha --compilers ts:ts-node/register test/",
    "precover": "rm -fr coverage && rm -fr .nyc_output",
    "cover": "nyc --extension .ts npm test",
    "postcover": "nyc report --reporter=text-lcov && rm -fr .nyc_output/",
    "precoveralls": "rm -fr coverage && rm -fr .nyc_output",
    "coveralls": "nyc --extension .ts npm test",
    "postcoveralls": "nyc report --reporter=text-lcov | node scripts/fix-sourcemaps.js | coveralls"
  },
  "license": "MIT",
  "dependencies": {
    "es6-shim": "^0.35.1",
    "redux": "^3.2.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.11",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "must": "^0.13.1",
    "nyc": "^7.0.0",
    "sinon": "^1.17.3",
    "ts-loader": "^0.8.0",
    "ts-node": "^1.0.0",
    "typescript": "^1.7.5",
    "typings": "^1.3.1",
    "webpack": "^1.12.12"
  },
  "description": "A ridiculously good syntax for working with Redux and TypeScript.  Currently limited to Angular 2 but could potentially be used elsewhere.",
  "main": "src/redux-decorators.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/KarlPurk/redux-decorators.git"
  },
  "keywords": [
    "redux",
    "typescript",
    "angular2"
  ],
  "author": "Karl Purkhardt",
  "bugs": {
    "url": "https://github.com/KarlPurk/redux-decorators/issues"
  },
  "homepage": "https://github.com/KarlPurk/redux-decorators"
}
